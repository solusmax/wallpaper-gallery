{
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --open --host",
    "build": "npm run lint && sh bash/build.sh -s && vite build && sh bash/build.sh -e",
    "build:demo": "sh bash/build.sh -s && vite build --minify false && sh bash/build.sh -e",
    "backup-project-json": "sh bash/build.sh -c",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "npm run build && vite preview --open"
  },
  "dependencies": {
    "decode-uri-component": "^0.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-photo-album": "^2.3.0",
    "yet-another-react-lightbox": "^3.12.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.22",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.49.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "postcss": "^8.4.30",
    "tailwindcss": "^3.3.3",
    "vite": "^4.4.9"
  }
}
